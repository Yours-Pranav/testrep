# Arbitrum RPC (e.g. https://arb1.arbitrum.io/rpc or Alchemy/Infura endpoint for Arbitrum)
RPC_URL=https://arb1.example.rpc

# Admin private key (NEVER commit; set this in Vercel env vars)
ADMIN_PRIVATE_KEY=__REPLACE__

# Addresses
NFT_CONTRACT_ADDRESS=__REPLACE_IF_ALREADY_DEPLOYED__
REKT_TOKEN_ADDRESS=__REPLACE_WITH_YOUR_ERC20_ADDRESS__

# nft.storage api key to pin metadata (optional but recommended)
NFT_STORAGE_KEY=__OPTIONAL__

# Warpcast / Farcaster integration (optional)
WARPSERVER_API_KEY=__OPTIONAL__
WARPSERVER_SIGNER_UUID=__OPTIONAL__

# Maximum tokens to send on mint (admin controlled in UI too)
MAX_REKT_PER_MINT=100
